@import org.shuzu.generator.*
@extends("layout")
@args Site site

<div id="orgs">
    <div>
        @for(Organization org: site.getOrgs()) {
        <div>
            <div><a href="/@site.orgPath(org)">@org.getName()</a></div>
            <div>@org.repos.getSize() demos</div>
        </div>
        }
    </div>
</div>
